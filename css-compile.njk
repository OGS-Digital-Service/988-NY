.hhs-dark {
    background-color: #43285D;
}
.hhs-mid {
    background-color: #8b7f9f;
}
.hhs-light {
    background-color: #d3d5e3;
}
.nysds-page {
    margin: 30px 10%;
    
}

.nysds-page p {
    font-size: 16px;
    color: #000;
}

.nysds-page ul {
    font-size: 16px;
    color: #000;
}
.nysds-page a {
    text-decoration: underline;
    color: #0050DB;
}

.nysds-page a:hover {
    color: #154973;
}

.nysds-page a:focus {
    color: #154973;
}

